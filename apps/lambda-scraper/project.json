{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/lambda-scraper/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/lambda-scraper",
        "main": "apps/lambda-scraper/src/main.js",
        "tsConfig": "apps/lambda-scraper/tsconfig.app.json",
        "externalDependencies": "none"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/lambda-scraper/src/environments/environment.js",
              "with": "apps/lambda-scraper/src/environments/environment.prod.js"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "lambda-scraper:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "lambda-scraper:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/lambda-scraper/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/lambda-scraper"],
      "options": {
        "jestConfig": "apps/lambda-scraper/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
